   -
     code : |1-
      
      Procedure WIN_Install(ManualMode = False)
      
      // Manual mode (for installing or uninstalling)
      IF ManualMode THEN
      	
      	// Checks whether the service exists
      	IF ServiceExist(SERVICE_NAME) THEN
      		
      		// Proposes to uninstall the service
      		STC_SERVICEINFO = “The service is installed.”
      		BTN_INSTALL..Caption = “Un&install”
      	ELSE
      		
      		// Proposes to install the service
      		STC_SERVICEINFO = “The service is not installed.”
      		BTN_INSTALL..Caption = “&Install”			
      	END
      	
      	// Plane 2	
      	WIN_Install..Plane = 2
      ELSE
      	
      	// Default plane
      	WIN_Install..Plane = 1
      END
